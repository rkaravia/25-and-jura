<?xml version='1.0' encoding='utf-8'?>
<widget id="ch.karavia.twenty_five_and_jura" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>25 and Jura</name>
    <description>
        Show the cantons around you using swissBOUNDARIES3D canton boundaries.
    </description>
    <author>
        Roman Karavia
    </author>
    <content src="index.html" />
    <icon src="src/img/25-and-jura.png" />
    <plugin name="cordova-plugin-whitelist" spec="1" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <platform name="android">
        <allow-intent href="market:*" />
    </platform>
    <platform name="ios">
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
    </platform>
    <plugin name="cordova-plugin-geolocation" spec="^4.0.1" />
    <engine name="browser" spec="^6.0.0" />
    <engine name="android" spec="^7.1.4" />
</widget>
